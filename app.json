{
  "expo": {
    "name": "Sertifika",
    "slug": "Sertifika v.2",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "sertifikaV.2",
      "permissions" : ["CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Example of React Native Bottom  Navigation"
  }
}
